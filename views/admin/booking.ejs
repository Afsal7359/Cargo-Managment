<div class="page">
    <div class="page-main">
        <!--app-content open-->
        <div class="app-content main-content mt-0">
            <div class="side-app">
                <!-- CONTAINER -->
                <div class="main-container container-fluid">
                    <!-- PAGE-HEADER -->
                    <div class="page-header">
                        <div>
                            <h1 class="page-title">Blog Page</h1>
                        </div>
                        <div class="ms-auto pageheader-btn">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="javascript:void(0);">Blog Page</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Blog</li>
                            </ol>
                        </div>
                    </div>
                    <!-- PAGE-HEADER END -->

                    <% if (bookings) { %>
                    <div class="row">
                        <div class="row row-sm">
                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="card-header border-bottom">
                                        <h3 class="card-title">Bookings Page Datatable</h3>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table id="editable-responsive-table" class="table editable-table table-nowrap table-bordered table-edit wp-100">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>From</th>
                                                        <th>Destination</th>
                                                        <th>N.O of Box</th>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Number</th>
                                                        <th>Details</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% for (let i = 0; i < bookings.length; i++) { %>
                                                        <% let currentBooking = bookings[i]; %>
                                                        <tr>
                                                            <td><%= i + 1 %></td>
                                                            <td><%= currentBooking.from %></td>
                                                            <td><%= currentBooking.destination %></td>
                                                            <td><%= currentBooking.box %></td>
                                                            <td><%= currentBooking.name %></td>
                                                            <td><%= currentBooking.email %></td>
                                                            <td><%= currentBooking.number %></td>
                                                            <td><%= currentBooking.details %></td>
                                                        </tr>
                                                    <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Row -->
                    <% } else { %>
                        <h1 style="color: red;">Table not found</h1>
                    <% } %>
                </div>
            </div>
        </div>
        <!-- CONTAINER CLOSED -->
    </div>
</div>
